<?xml version="1.0" encoding="UTF-8"?>
<xliff xmlns:t3="http://typo3.org/schemas/xliff" version="1.0">
  <file t3:id="1415814893" source-language="en" datatype="plaintext" original="EXT:brofix/Resources/Private/Language/locallang.xlf" date="2011-10-17T20:22:34Z" product-name="brofix" target-language="de">
    <header/>
    <body>
      <trans-unit id="mod_brofix" resname="mod_brofix">
        <source>Broken link fixer</source>
        <target state="translated">Links prüfen</target>
      </trans-unit>
      <!--
				__________________
				broken links email
				__________________
			-->
      <trans-unit id="email.brokenlinksreport.title" resname="email.brokenlinksreport.title">
        <source>Broken Links Report</source>
        <target state="translated">Bericht über fehlerhafte Links</target>
      </trans-unit>
      <!-- header is in subject and used as header in email body -->
      <trans-unit id="email.brokenlinksreport.header" resname="email.brokenlinksreport.header">
        <source>Broken links for page</source>
        <target state="translated">Fehlerhafte Links für Seite</target>
      </trans-unit>
      <trans-unit id="email.brokenlinksreport.header.broken_links" resname="email.brokenlinksreport.header.broken_links">
        <source>Broken links</source>
        <target state="translated">Fehlerhafte Links</target>
      </trans-unit>
      <trans-unit id="email.brokenlinksreport.percent_broken_links" resname="email.brokenlinksreport.percent_broken_links">
        <source>Percent broken links</source>
        <target state="translated">Fehlerhafte Links in Prozent</target>
      </trans-unit>
      <trans-unit id="email.brokenlinksreport.number_broken_links" resname="email.brokenlinksreport.number_broken_links">
        <source>Number of broken links</source>
        <target state="translated">Anzahl fehlerhafte Links</target>
      </trans-unit>
      <trans-unit id="email.brokenlinksreport.number_links_checked" resname="email.brokenlinksreport.number_links_checked">
        <source>Total links checked</source>
        <target state="translated">Anzahl Links (gesamt)</target>
      </trans-unit>
      <trans-unit id="email.brokenlinksreport.header.pages" resname="email.brokenlinksreport.header.pages">
        <source>Pages checked</source>
        <target state="translated">Seiten</target>
      </trans-unit>
      <trans-unit id="email.brokenlinksreport.depth" resname="email.brokenlinksreport.depth">
        <source>Depth</source>
        <target state="translated">Tiefe</target>
      </trans-unit>
      <trans-unit id="email.brokenlinksreport.number_of_pages" resname="email.brokenlinksreport.number_of_pages">
        <source>Number of pages checked</source>
        <target state="translated">Anzahl Seiten</target>
      </trans-unit>
      <trans-unit id="email.brokenlinksreport.depth.infinite" resname="email.brokenlinksreport.depth.infinite">
        <source>infinite</source>
        <target state="translated">unendlich</target>
      </trans-unit>
      <trans-unit id="email.brokenlinksreport.header.excluded_urls" resname="email.brokenlinksreport.header.excluded_urls">
        <source>Excluded URLs</source>
        <target state="translated">Von Prüfung ausgeschlossene URLs</target>
      </trans-unit>
      <trans-unit id="email.brokenlinksreport.links_not_checked" resname="email.brokenlinksreport.links_not_checked">
        <source>Links not checked</source>
        <target state="translated">Anzahl Links</target>
      </trans-unit>
      <trans-unit id="email.brokenlinksreport.links_not_checked_percent" resname="email.brokenlinksreport.links_not_checked_percent">
        <source>Percent links not checked</source>
        <target state="translated">Anzahl Links in Prozent</target>
      </trans-unit>
      <trans-unit id="email.brokenlinksreport.header.check" resname="email.brokenlinksreport.header.check">
        <source>Check</source>
        <target state="translated">Prüfung</target>
      </trans-unit>
      <trans-unit id="email.brokenlinksreport.check.start" resname="email.brokenlinksreport.check.start">
        <source>Check started</source>
        <target state="translated">Gestarted</target>
      </trans-unit>
      <trans-unit id="email.brokenlinksreport.check.done" resname="email.brokenlinksreport.check.done">
        <source>Check done</source>
        <target state="translated">Beendet</target>
      </trans-unit>
      <!-- broken link list -->
      <trans-unit id="list.header" resname="list.header">
        <source>Broken links</source>
        <target state="translated">Fehlerhafte Links</target>
      </trans-unit>
      <trans-unit id="label_refresh" resname="label_refresh">
        <source>Refresh display</source>
        <target state="translated">Anzeige aktualisieren</target>
      </trans-unit>
      <trans-unit id="label_refresh-link-list" resname="label_refresh-link-list">
        <source>Refreshing link list, please stand by.</source>
        <target state="translated">Linkliste wird aktualisiert.</target>
      </trans-unit>
      <trans-unit id="list.button.checkLinks" resname="list.button.checkLinks">
        <source>Check Links</source>
        <target state="translated">Links prüfen</target>
      </trans-unit>
      <trans-unit id="list.action.checkLinks" resname="list.action.checkLinks">
        <source>Checking Links. This may take a while.</source>
        <target state="translated">Links werden geprüft. Dies kann eine Weile dauern.</target>
      </trans-unit>
      <trans-unit id="list.edit.field" resname="list.edit.field">
        <source>Edit field containing this broken link</source>
        <target state="translated">Editiere Feld, welches den fehlerhaften link enthält</target>
      </trans-unit>
      <trans-unit id="list.action.web_layout" resname="list.action.web_layout">
        <source>Page layout</source>
        <target state="translated">Seiten-Layout</target>
      </trans-unit>
      <trans-unit id="list.action.recheckUrl" resname="list.action.recheckUrl">
        <source>Recheck URL</source>
        <target state="translated">URL neu prüfen</target>
      </trans-unit>
      <trans-unit id="list.action.excludeUrl" resname="list.action.excludeUrl">
        <source>Permanently exclude this URL from URL checking</source>
        <target state="translated">URL dauerhaft von Linkprüfung ausschließen</target>
      </trans-unit>
      <trans-unit id="broken_links_found_for_page" resname="broken_links_found_for_page">
        <source>Broken links were found on this page</source>
        <target state="translated">Fehlerhafte Links auf dieser Seite gefunden</target>
      </trans-unit>
      <trans-unit id="count_singular_broken_links_found_for_page" resname="count_broken_links_found_for_page">
        <source>%d broken link was found on this page.</source>
        <target state="translated">%d fehlerhafter Link wurde auf dieser Seite gefunden.</target>
      </trans-unit>
      <trans-unit id="count_plural_broken_links_found_for_page" resname="count_broken_links_found_for_page">
        <source>%d broken links were found on this page.</source>
        <target state="translated">%d fehlerhafte Links wurden auf dieser Seite gefunden.</target>
      </trans-unit>
      <trans-unit id="go_to_info_modul" resname="go_to_info_modul">
        <source>Go to the Info module</source>
        <target state="translated">Zum Info Modul wechseln</target>
      </trans-unit>
      <trans-unit id="and_check_links" resname="and_check_links">
        <source>and check links</source>
        <target state="translated">und Links prüfen</target>
      </trans-unit>
    </body>
  </file>
</xliff>
